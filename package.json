{
  "name": "react-slite",
  "version": "0.1.5",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "license": "MIT",
  "author": "palerdot<palerdot@gmail.com>",
  "repository": "https://github.com/palerdot/react-slite",
  "module": "dist/react-slite.esm.js",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=14"
  },
  "peerDependencies": {
    "react": ">=17"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "size-limit": [
    {
      "path": "dist/react-slite.cjs.production.min.js",
      "limit": "160 KB"
    },
    {
      "path": "dist/react-slite.esm.js",
      "limit": "110 KB"
    }
  ],
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "vitest",
    "lint": "prettier --list-different src stories test",
    "lint:fix": "prettier --write src stories test",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "devDependencies": {
    "@size-limit/preset-small-lib": "^8.2.4",
    "@storybook/addon-essentials": "^7.0.7",
    "@storybook/addon-info": "^5.3.21",
    "@storybook/addon-links": "^7.0.7",
    "@storybook/addons": "^7.0.7",
    "@storybook/react": "^7.0.7",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.1",
    "babel-loader": "^9.1.2",
    "happy-dom": "^9.10.1",
    "husky": "^8.0.3",
    "prettier": "^2.8.8",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-is": "^18.2.0",
    "size-limit": "^8.2.4",
    "tsdx": "^0.14.1",
    "tslib": "^2.5.0",
    "typescript": "^5.0.4",
    "vitest": "^0.30.1"
  }
}
